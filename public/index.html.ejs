<!DOCTYPE html>
<html lang="en" dir="ltr">
    <head>
        <meta charset="UTF-8"/>
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <title>Hello React!</title>
    </head>
    <body>
        <div id="app"></div>

        <!-- Dependencies -->
        <% if (webpackConfig.mode == 'development') { %>
            <script crossorigin src="https://cdnjs.cloudflare.com/ajax/libs/react/16.8.2/umd/react.development.jss"></script>
            <script crossorigin src="https://cdnjs.cloudflare.com/ajax/libs/react-dom/16.8.2/umd/react-dom.development.js"></script>
        <% } else { %>
            <script crossorigin src="https://cdnjs.cloudflare.com/ajax/libs/react/16.8.2/umd/react.production.min.js"></script>
            <script crossorigin src="https://cdnjs.cloudflare.com/ajax/libs/react-dom/16.8.2/umd/react-dom.production.min.js"></script>
        <% } %>
    </body>
</html>
