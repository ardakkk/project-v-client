{
    "presets": [
        ["@babel/preset-env", { "modules": false }],
        "@babel/preset-react"
    ],
    "plugins": [
        "react-hot-loader/babel", 
        "@babel/plugin-syntax-dynamic-import",
        "react-loadable/babel"
    ],
    "env": {
        "production": {
            "presets": ["minify"]
        },
        "test": {
            "presets": ["@babel/preset-env", "@babel/preset-react", "react-hot-loader/babel"]
        }
    }
}
