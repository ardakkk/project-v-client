{
  "name": "react-pwa-typescript",
  "version": "1.0.0",
  "description": "react with typescript and pwa",
  "main": "index.js",
  "scripts": {
    "prod": "export NODE_ENV=prod && npm run clean-build && webpack -p --config ./configs/webpack/webpack.prod.js",
    "start": "export NODE_ENV=development && npm run start-dev",
    "start-dev": "webpack-dev-server --hot --open --progress --inline --config ./configs/webpack/webpack.dev.js",
    "start-prod": "npm run prod && node server.js",
    "clean-build": "rm -f -r -d build",
    "test": "jest --watch --coverage --config=configs/jest.json"
  },
  "author": "Ardak Tileu",
  "license": "ISC",
  "dependencies": {
    "@hot-loader/react-dom": "npm:react-dom",
    "@types/node": "^11.9.4",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.1",
    "express": "^4.16.4",
    "react": "^16.8.2",
    "react-dom": "^16.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@types/enzyme": "^3.1.18",
    "@types/enzyme-adapter-react-16": "^1.0.4",
    "@types/jest": "^24.0.6",
    "autoprefixer": "^9.4.7",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^24.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.6.5",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3",
    "webpack": "^4.29.4",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  }
}
